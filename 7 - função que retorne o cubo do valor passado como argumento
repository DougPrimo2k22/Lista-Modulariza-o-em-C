#include <stdio.h>

float cubo(float valor);

int main(){
    float valor;
    
    printf("Digite o numero que deseja elevar ao cubo: ");      //Mensagem de entrada
    scanf("%f",&valor);
    cubo(valor);
    
}
    float cubo(float valor){                                  //Função que calcula valor ao cubo
   
     printf(" ao cubo é %.2f",(valor * valor)*valor);
     return 0;
      
    }
